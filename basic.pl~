#!perl
BEGIN {
    @DB::typeahead = (
	'x \%DB::sub',
	'x \@DB::sub',
	'b d',
	'c',
	#'use Devel::Peek; Dump(\%DB::sub)',
	'x [ defined &d ]',
	'x [ DB::CvGV_name_or_bust("d") ]',
	'$main::Caught = $main::Value',
	'c',
	'q'
    );
}

sub a { $main::Caught = 1 }
sub b { $main::Caught = 2 }
sub c { $main::Caught = 3 }

sub d { $main::Caught = 4 }

sub e { $main::Caught = 5 }
sub f { $main::Caught = 6 }
sub g { $main::Caught = 7 }

use Enbugger;
Enbugger->stop;
a();
b();
c();
d();
e();
f();
g();

print "\$main::Caught = $main::Caught.\n";
